{
  "manifest_version": 3,
  "name": "Shapr3D Export Helper",
  "version": "1.69",


  "content_scripts": [
    {
      "matches": ["*://app.shapr3d.com/*"],
      "run_at": "document_start",
      "js": ["content.js"],
      "css": ["custom.css"]
    }
  ],

  "web_accessible_resources": [
    {
      "resources": ["injected.js"],
      "matches": ["*://app.shapr3d.com/*"]
    }
  ]
}
